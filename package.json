{
  "name": "js-ddd",
  "version": "1.0.2",
  "description": "JS simple console logging like a Kint for php",
  "main": "./dist/ddd.js",
  "scripts": {
    "clean": "rm -rf ./example/build",
    "init": "npm install",
    "release": "rm -rf ./dist; webpack --config webpack.config.dist.js",
    "start": "npm run clean; webpack",
    "start:hard": "rm -rf ./node_modules; npm run init; npm run start",
    "watch": "npm run start; webpack-dev-server --hot --inline"
  },
  "author": "Bogdan Bodnaruk",
  "license": "ISC",
  "dependencies": {
    "kind-of": "^3.0.2"
  },
  "devDependencies": {
    "react": "^0.13",
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.2",
    "file-loader": "^0.8.5",
    "react-hot-loader": "^1.3.0",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.10.1"
  },
  "npmName": "js-ddd",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
